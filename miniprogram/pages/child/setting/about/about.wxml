    <van-cell size="large" title="隐私权政策" url="{{pages.PSettingPrivacy}}" is-link clickable />
    <van-cell border="{{false}}" size="large" title="服务协议" url="{{pages.PSettingTerms}}" is-link clickable />
</van-cell-group>
